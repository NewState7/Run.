-- ESP Logic
local espTags = {}

local function createEspTag(player)
	if espTags[player] then return end
	if not player.Character then return end
	local hrp = player.Character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	local highlight = Instance.new("Highlight")
	highlight.Adornee = player.Character
	highlight.FillColor = Color3.fromRGB(0, 255, 0)
	highlight.OutlineColor = Color3.fromRGB(0, 255, 0)
	highlight.Parent = workspace
	espTags[player] = highlight
	
	local billboard = Instance.new("BillboardGui")
	billboard.Name = "EspBillboard"
	billboard.Adornee = hrp
	billboard.Size = UDim2.new(0, 120, 0, 40)
	billboard.StudsOffset = Vector3.new(0, 2.5, 0)
	billboard.AlwaysOnTop = true
	billboard.Parent = hrp
	
	local textLabel = Instance.new("TextLabel")
	textLabel.BackgroundTransparency = 1
	textLabel.Size = UDim2.new(1, 0, 1, 0)
	textLabel.Font = Enum.Font.GothamBold
	textLabel.TextSize = 14
	textLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
	textLabel.TextStrokeTransparency = 0
	textLabel.Parent = billboard
	
	task.spawn(function()
		while espEnabled and player.Character and player.Character:FindFirstChild("Humanoid") and billboard.Parent do
			local humanoid = player.Character.Humanoid
			local hrp = player.Character:FindFirstChild("HumanoidRootPart")
			local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
			if humanoid and hrp and localHRP then
				local distance = (localHRP.Position - hrp.Position).Magnitude
				local hp = humanoid.Health
				local maxHp = humanoid.MaxHealth
				textLabel.Text = string.format("%s\nHP: %d/%d\nKM: %.2f", player.Name, hp, maxHp, distance/1000)
			else
				textLabel.Text = player.Name
			end
			task.wait(0.5)
		end
		if billboard then billboard:Destroy() end
	end)
end

local function removeEspTag(player)
	if espTags[player] then
		if espTags[player].Parent then
			espTags[player]:Destroy()
		end
		espTags[player] = nil
	end
end

local function updateESP()
	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer then
			if espEnabled then
				if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
					createEspTag(player)
				end
			else
				removeEspTag(player)
			end
		end
	end
end

Players.PlayerAdded:Connect(function(player)
	if espEnabled then
		player.CharacterAdded:Connect(function()
			task.wait(0.5)
			if espEnabled then
				createEspTag(player)
			end
		end)
	end
end)

Players.PlayerRemoving:Connect(function(player)
	removeEspTag(player)
end)

-- Fly + noclip + bay tới player
RunService.RenderStepped:Connect(function(dt)
	if flying and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
		local hrp = LocalPlayer.Character.HumanoidRootPart
		
		-- Noclip tự động bật khi bay
		for _, part in pairs(LocalPlayer.Character:GetChildren()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
		
		if flyToTarget and flyToTarget.Character and flyToTarget.Character:FindFirstChild("HumanoidRootPart") then
			local targetPos = flyToTarget.Character.HumanoidRootPart.Position
			local direction = (targetPos - hrp.Position)
			local distance = direction.Magnitude
			if distance > 3 then
				local moveVec = direction.Unit * flySpeed * dt
				LocalPlayer.Character:TranslateBy(moveVec)
			else
				tpMessage.TextColor3 = Color3.fromRGB(0, 255, 0)
				tpMessage.Text = "Đã đến gần " .. flyToTarget.Name
				flyToTarget = nil
			end
		else
			flyToTarget = nil
			local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
			if humanoid then
				local moveDirection = humanoid.MoveDirection
				if moveDirection.Magnitude > 0 then
					LocalPlayer.Character:TranslateBy(moveDirection * flySpeed / 100)
				end
			end
		end
	else
		if LocalPlayer.Character then
			for _, part in pairs(LocalPlayer.Character:GetChildren()) do
				if part:IsA("BasePart") then
					part.CanCollide = not noclipEnabled
				end
			end
		end
	end
end)

-- Button events
local function setFlying(state)
	flying = state
	if flying then
		toggleFlyBtn.Text = "Run: ON"
		toggleFlyBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
	else
		toggleFlyBtn.Text = "Run: OFF"
		toggleFlyBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
		flyToTarget = nil
	end
end

toggleFlyBtn.MouseButton1Click:Connect(function()
	setFlying(not flying)
end)

speedBtn.MouseButton1Click:Connect(function()
	if flySpeed >= 750 then
		flySpeed = 300
	else
		flySpeed += 25
	end
	speedBtn.Text = "Speed: " .. flySpeed
end)

seaToggleBtn.MouseButton1Click:Connect(function()
	seaStand = not seaStand
	if seaStand then
		seaToggleBtn.Text = "Sea: ON"
		seaToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 100, 255)
	else
		seaToggleBtn.Text = "Sea: OFF"
		seaToggleBtn.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
	end
end)

espToggleBtn.MouseButton1Click:Connect(function()
	espEnabled = not espEnabled
	if espEnabled then
		espToggleBtn.Text = "ESP: ON"
		espToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
	else
		espToggleBtn.Text = "ESP: OFF"
		espToggleBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
	end
	updateESP()
end)

noclipToggleBtn.MouseButton1Click:Connect(function()
	noclipEnabled = not noclipEnabled
	if noclipEnabled then
		noclipToggleBtn.Text = "Noclip: ON"
		noclipToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
	else
		noclipToggleBtn.Text = "Noclip: OFF"
		noclipToggleBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
	end
end)

tpButton.MouseButton1Click:Connect(function()
	local targetName = tpTextBox.Text
	if targetName == "" then
		tpMessage.TextColor3 = Color3.fromRGB(255, 100, 100)
		tpMessage.Text = "Vui lòng nhập tên người chơi!"
		flyToTarget = nil
		return
	end
	
	local targetPlayer = Players:FindFirstChild(targetName)
	if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
		tpMessage.TextColor3 = Color3.fromRGB(255, 100, 100)
		tpMessage.Text = "Không tìm thấy người chơi hoặc họ chưa vào game!"
		flyToTarget = nil
		return
	end
	
	tpMessage.TextColor3 = Color3.fromRGB(0, 255, 255)
	tpMessage.Text = "Đang bay tới " .. targetPlayer.Name .. "..."
	flyToTarget = targetPlayer
	setFlying(true)
end)

stopFlyToBtn.MouseButton1Click:Connect(function()
	if flyToTarget then
		flyToTarget = nil
		tpMessage.TextColor3 = Color3.fromRGB(255, 100, 100)
		tpMessage.Text = "Đã dừng bay tới người chơi"
	end
end)

-- Platform đứng trên biển
local waterPart = Instance.new("Part")
waterPart.Size = Vector3.new(1000, 1, 1000)
waterPart.Anchored = true
waterPart.Transparency = 1
waterPart.CanCollide = true
waterPart.Parent = workspace

RunService.Heartbeat:Connect(function()
	if seaStand and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
		local pos = LocalPlayer.Character.HumanoidRootPart.Position
		waterPart.Position = Vector3.new(pos.X, -2, pos.Z)
	else
		waterPart.Position = Vector3.new(999999, -5000, 999999)
	end
end)

